<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuforPlayController">
   <children>
      <Button fx:id="closeButton" layoutX="203.0" layoutY="314.0" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="42.0" prefWidth="195.0" text="quitter" />
