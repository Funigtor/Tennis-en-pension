          <MenuItem fx:id="MatchSolo" mnemonicParsing="false" text="Match amical" />
          <MenuItem fx:id="TournoiMatch" mnemonicParsing="false" text="tournoi" />
            <MenuItem fx:id="YearMatchs" mnemonicParsing="false" text="Annee" />
        </items>
      </MenuButton>
      <Label alignment="CENTER" layoutX="108.0" layoutY="42.0" prefHeight="32.0" prefWidth="385.0" text="TENNIS-EN-PENSION" textAlignment="CENTER">
