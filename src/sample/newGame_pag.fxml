<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewGamePagController">
   <children>
      <Button fx:id="oneByOne" layoutX="103.0" layoutY="173.0" mnemonicParsing="false" onAction="#nextPlayer" prefHeight="41.0" prefWidth="195.0" text="create One by one" />
      <Label layoutX="156.0" layoutY="36.0" text="New Game">
         <font>
            <Font name="Droid Sans Fallback" size="16.0" />
         </font>
      </Label>
      <Button fx:id="randCreat" layoutX="102.0" layoutY="132.0" mnemonicParsing="false" onAction="#randomCreatF" prefHeight="41.0" prefWidth="195.0" text="Generate random list" />
      <Button fx:id="loadList" layoutX="103.0" layoutY="91.0" mnemonicParsing="false" onAction="#loadListe" prefHeight="41.0" prefWidth="195.0" text="Load list of player" />
      <Button fx:id="back" layoutX="103.0" layoutY="307.0" mnemonicParsing="false" onAction="#goBack" prefHeight="41.0" prefWidth="195.0" text="Back" />
      <TextField fx:id="date" layoutX="90.0" layoutY="259.0" />
      <Text layoutX="121.0" layoutY="245.0" strokeType="OUTSIDE" strokeWidth="0.0" text="What year will we play?">
         <font>
            <Font name="DejaVu Serif Bold" size="12.0" />
         </font>
      </Text>
      <Button fx:id="valider" layoutX="261.0" layoutY="259.0" mnemonicParsing="false" onAction="#setYear" prefHeight="26.0" prefWidth="48.0" text="Ok" />
      <Label fx:id="erreur" layoutX="114.0" layoutY="319.0" prefHeight="16.0" prefWidth="171.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
